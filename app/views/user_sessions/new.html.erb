<%= form_for(:user_session) do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit %>
<% end %>

<% if @msg %>
  <div class="msg"><%= @msg %></div>
<% end %>

<% if @url %>
  <%= link_to(@url, @url) %>
<% end %>
