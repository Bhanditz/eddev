function buildSidebar() {
  //"label": "Cooper's Hawk",
  //"sciname": "Accipiter cooperii (Bonaparte, 1828)"

  var enrichedNodes = []
    , speciesList = $('#SPECIES_LIST');

  $.each(interactions.nodes, function(index, node) {
    var item = speciesList.append(
      '<li class="species" data-id="' + node.id + '">' +
        '<div class="speciesThumbWrap">' + 
          '<img class="speciesThumb" alt="" src="' + thumbnailUrls[node.id] + '">' +
        '</div>' +
        '<div class="commonName">' + node.label + '</div>' +
//        '<div class="sciName">' + node.sciname + '</div>' +
      '</li>' 
    );
  }); 

  $('.species').click(function() {
    var id = $(this).data('id')
      , node = cy.getElementById(id);

    node.trigger('tap');
  });
}

function buildElements() {
  var elements = [];

  // Add nodes
  $.each(interactions.nodes, function(index, node) {
    elements.push({ data: { id: node.id, label: node.label } }); 
  });

  // Add edges
  $.each(interactions.links, function(index, edge) {
      elements.push({ data: { 
        id: edge.from + "-" + edge.to,
        source: edge.from,
        target: edge.to
      }});
  });

  return elements;
}

function buildStyle() {
  var style = [{
    selector: 'node',
    style: {
      'label': 'data(label)',
      'background-fit': 'cover',
      'height': 80,
      'width': 80,
      'border-color': '#000',
      'border-width': 2
    },
  },
  {
    selector: 'node.selected',
    style: {
      'border-color': '#6666ff',
      'border-width': 6,
    }
  },
  {
    selector: 'edge',
    style: {
      "target-arrow-shape": 'triangle'
    }
  },
  {
    selector: 'edge.feeds, edge.eats',
    style: {
      'width': 6
    }
  },
  {
    selector: 'edge.feeds',
    style: {
      'line-color': '#ff8888',
      'target-arrow-color': '#ff8888',
    }
  },
  {
    selector: 'edge.eats',
    style: {
      'line-color': '#88ff88',
      'target-arrow-color': '#88ff88' 
    }
  }
  ];

  for (var key in thumbnailUrls) {
    style.push({
      selector: '#' + key,
      style: {
        'background-image': thumbnailUrls[key]
      }
    });
  }

  return style;
}

function foodChain() {
  var selected = this.hasClass('selected');

  cy.nodes().removeClass('selected');
  cy.edges().removeClass('feeds');
  cy.edges().removeClass('eats');

  if (selected) {
    return;
  } 

  this.addClass('selected');
   
  // dfs on outgoing edges
  cy.elements().dfs({
    root: this,
    visit: function(i, depth, v, e, u) {
      if (e) {
        e.addClass('feeds');
      }
    },
    directed: true
  });

  // bfs on incoming edges (not supported by cy)
  var nodes = [this];
  var visited = [];
  var node = null;
  var edges = null;
  while (nodes.length) {
    node = nodes.pop();

    edges = node.connectedEdges(function() {
      return this.target().same(node) && !this.target().anySame(visited);
    });

    visited.push(node);
    edges.addClass('eats');

    $.each(edges, function(index, edge) {
      nodes.unshift(edge.source())
    })
  }
}

$(function() {
  var layoutBf = {
    name: 'breadthfirst',
    spacingFactor: 1,
    animate: true,
    animationDuration: 500,
    circle: true,
    directed: true
  };

  var layoutGrid = {
    name: 'grid',
    animate: true,
    animationDuration: 500,
    directed: true
  };

  window.cy = cytoscape({
    container: document.getElementById('FOODWEB_CONTAINER'),
    elements: buildElements(),
    style: buildStyle(),
    layout: layoutBf
  });

  $('#LAYOUT_BF').click(function() {
    cy.layout(layoutBf);
  });

  $('#LAYOUT_GRID').click(function() {
    cy.layout(layoutGrid);
  });

  cy.on('tap', 'node', foodChain);

  buildSidebar();
});
</script>
